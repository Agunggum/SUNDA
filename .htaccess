<FilesMatch "\.?htaccess$">
 order allow,deny
 deny from all
</FilesMatch>

<FilesMatch "\.?txt$">
 order allow,deny
 deny from all
</FilesMatch>

RewriteEngine on

# Disable directory browsing
Options -Indexes
